{
  "apiVersion": "apps/v1",
  "kind": "Deployment",
  "metadata": {
    "name": "release-webfrontendchart",
    "labels": {
      "helm.sh/chart": "webfrontendchart-0.1.0",
      "app.kubernetes.io/name": "webfrontendchart",
      "app.kubernetes.io/instance": "release",
      "app.kubernetes.io/version": "v1",
      "app.kubernetes.io/managed-by": "Helm"
    }
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "matchLabels": {
        "app.kubernetes.io/name": "webfrontendchart",
        "app.kubernetes.io/instance": "release"
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "helm.sh/chart": "webfrontendchart-0.1.0",
          "app.kubernetes.io/name": "webfrontendchart",
          "app.kubernetes.io/instance": "release",
          "app.kubernetes.io/version": "v1",
          "app.kubernetes.io/managed-by": "Helm"
        }
      },
      "spec": {
        "serviceAccountName": "release-webfrontendchart",
        "securityContext": {},
        "containers": [
          {
            "name": "webfrontendchart",
            "securityContext": {
              "capabilities": {
                "drop": [
                  "ALL"
                ]
              },
              "readOnlyRootFilesystem": true,
              "runAsNonRoot": false,
              "runAsUser": 1000,
              "seccompProfile": {
                "type": "RuntimeDefault"
              }
            },
            "image": "amyacr.azurecr.io/webfrontend:v1",
            "imagePullPolicy": "IfNotPresent",
            "ports": [
              {
                "name": "http",
                "containerPort": 80,
                "protocol": "TCP"
              }
            ],
            "livenessProbe": {
              "httpGet": {
                "path": "/",
                "port": "http"
              }
            },
            "readinessProbe": {
              "httpGet": {
                "path": "/",
                "port": "http"
              }
            },
            "resources": {}
          }
        ]
      }
    }
  }
}
